# Variable assignment
# snake_case preferred
# var_name is value
# or var_name = value
# 'is' syntax is idiomatic, but choose whatever reads better
dog_breed is "Corgi"
corgis_age is 3
long_legs is false
numbers = [1, 2, 3]
# Try to use grammar that is consistent with English

# Constants
# SCREAMING_SNAKE_CASE preferred
# CONST is value
SERVER_IP is 'localhost'
NUM_RETRIES is 17
